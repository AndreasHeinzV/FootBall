<html>
{% block header %}
    {% include 'base.twig' %}
{% endblock %}
<body>
{% if teams is not empty %}
    <table>
        <tr>
            <th>Position</th>
            <th>Team</th>
            <th>Played</th>
            <th>Won</th>
            <th>Draw</th>
            <th>Lost</th>
            <th>Points</th>
            <th>GoalsFor</th>
            <th>GoalsAgainst</th>
            <th>GoalsDifference</th>
        </tr>
        {% for team in teams %}
            <tr>
                <td>{{ team.position }} </td>
                <td><a href="{{ team.link }}">{{ team.name }}</a></td>
                <td> {{ team.playedGames }}</td>
                <td> {{ team.won }} </td>
                <td>{{ team.draw }} </td>
                <td>{{ team.lost }}</td>
                <td>{{ team.points }}</td>
                <td>{{ team.goalsFor }}</td>
                <td>{{ team.goalsAgainst }}</td>
                <td>{{ team.goalDifference }}</td>
                <td>


                </td>


            </tr>
        {% endfor %}
    </table>
{% else %}
    <h1> Page not found return to homepage</h1>
    <a href="/">Index</a>
{% endif %}
</body>
</html>
